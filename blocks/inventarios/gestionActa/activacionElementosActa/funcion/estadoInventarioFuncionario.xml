<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Estado General Inventario Funcionario</ReportTitle><ReportDescription>Seleccione criterios de b√∫squeda</ReportDescription><PageSize>.DEFAULT</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize></pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><gridDisplay>.DEFAULT</gridDisplay><gridSortable>.DEFAULT</gridSortable><gridSearchable>.DEFAULT</gridSearchable><gridPageable>.DEFAULT</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>mysql</DatabaseType><DatabaseName></DatabaseName><HostName></HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT DISTINCT 
fecha_inicio, 
fecha_final,
sa.&quot;ESF_SEDE&quot; sede,
ad.&quot;ESF_DEP_ENCARGADA&quot; dependencia, 
&quot;FUN_IDENTIFICACION&quot; identificacion,&quot;FUN_NOMBRE&quot; nombre_funcionario,
sum(CASE WHEN confirmada_existencia=TRUE THEN 0 WHEN confirmada_existencia=false THEN 1 END) confirmacion,
CASE WHEN sum(CASE WHEN confirmada_existencia=TRUE THEN 0 WHEN confirmada_existencia=false THEN 1 END)=0 THEN 'Aprobado' WHEN sum(CASE WHEN confirmada_existencia=TRUE THEN 0 WHEN confirmada_existencia=false THEN 1 END)&gt;0 THEN 'No aprobado' END estado_inventario,
coalesce((CASE WHEN estado_radicacion=TRUE THEN 'Radicado' END), 'No radicado')
 estado_radicaciones
FROM arka_inventarios.elemento_individual aei
JOIN arka_inventarios.elemento ele ON ele.id_elemento=aei.id_elemento_gen
JOIN arka_parametros.arka_funcionarios af ON af.&quot;FUN_IDENTIFICACION&quot;=aei.funcionario
LEFT JOIN historial_levantamiento_placa lp ON lp.placa=aei.placa
left JOIN arka_parametros.arka_espaciosfisicos ef ON  ef.&quot;ESF_ID_ESPACIO&quot;=aei.ubicacion_elemento
left JOIN arka_parametros.arka_dependencia as ad ON ad.&quot;ESF_ID_ESPACIO&quot;=ef.&quot;ESF_ID_ESPACIO&quot;
left JOIN arka_parametros.arka_sedes sa ON sa.&quot;ESF_COD_SEDE&quot;=ef.&quot;ESF_COD_SEDE&quot;
LEFT JOIN arka_movil.periodo_levantamiento pl ON pl.id_periodolevantamiento=lp.id_levantamiento
LEFT JOIN arka_movil.radicado_levantamiento mrl ON mrl.funcionario=aei.funcionario
WHERE 1=1
AND aei.placa!=''
AND pl.estado_registro=TRUE
GROUP BY &quot;ESF_SEDE&quot;,&quot;ESF_DEP_ENCARGADA&quot;, &quot;FUN_IDENTIFICACION&quot;,&quot;FUN_NOMBRE&quot;, estado_radicaciones, fecha_inicio, fecha_final
ORDER BY &quot;FUN_IDENTIFICACION&quot;</SQLRaw></SQL><QueryColumns><QueryColumn><Name>fecha_inicio</Name><TableName></TableName><ColumnName>DISTINCT 
